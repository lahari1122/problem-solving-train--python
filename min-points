def min(points):
   p=sorted(points,key=lambda x:x[1])
   end=points [0][1]
   arrow=1
   for i in range(1,len(points)):
        if(points[1][0]<=end):
            continue
        else:
            end=points[i][1]
            arrow=arrow+1
   return arrow
points=[(1,6),(2,4)]
res=min(points)
print(res)
